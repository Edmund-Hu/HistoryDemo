<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <display-name></display-name>
  <servlet>
    <servlet-name>FileUpload</servlet-name>
    <servlet-class>com.edmund.servlet.FileUpload</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>fileload</servlet-name>
    <servlet-class>com.edmund.servlet.fileload</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>image</servlet-name>
    <servlet-class>com.edmund.servlet.image</servlet-class>
  </servlet>



  <servlet-mapping>
    <servlet-name>FileUpload</servlet-name>
    <url-pattern>/FileUpload.edmund</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>fileload</servlet-name>
    <url-pattern>/fileload.edmund</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>image</servlet-name>
    <url-pattern>/image.edmund</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
