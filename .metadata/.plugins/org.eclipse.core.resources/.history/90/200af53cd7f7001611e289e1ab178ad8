		System.out.println("类加载之后的方法afterClass");
	}

	@Before
	public void setUp() throws Exception {
		System.out.println("测试方法执行之前before");
	}

	@After
	public void tearDown() throws Exception {
		System.out.println("测试方法执行之后after");
	}

	@Test
